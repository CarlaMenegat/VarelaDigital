@prefix hrao: <https://carlamenegat.github.io/VarelaDigital/hrao#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix san: <http://example.org/san#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rel: <http://purl.org/vocab/relationship/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

#################################################################
# Ontology header
#################################################################

hrao: a owl:Ontology ;
    owl:versionIRI <https://carlamenegat.github.io/VarelaDigital/hrao/1.0/> ;
    rdfs:label "Historical Relations and Agency Ontology"@en ;
    owl:versionInfo "1.0 (Varela Digital, 2026)" .


#################################################################
# Property: hrao:followedPoliticalAlignmentOf
#################################################################

hrao:followedPoliticalAlignmentOf a owl:ObjectProperty ;
    rdfs:label "followed the political alignment of"@en ;
    rdfs:comment "Indicates that an agent followed or aligned themselves with the political or strategic orientation of another agent, especially in contexts of shifting loyalties, leadership, or factional dynamics in historical settings."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:landDisputeWith
#################################################################

hrao:landDisputeWith a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "land dispute with"@en ;
    rdfs:comment "Indicates that two historical agents were engaged in a dispute over land, property boundaries, or territorial rights."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:requestedPoliticalFavorFrom
#################################################################

hrao:requestedPoliticalFavorFrom a owl:ObjectProperty ;
    rdfs:label "requested a political favor from"@en ;
    rdfs:comment "Indicates that an agent requested a political favor, intercession, or informal influence from another agent in a historical or political context."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:participatedInEvent
#################################################################

hrao:participatedInEvent a owl:ObjectProperty ;
    rdfs:label "participated in event"@en ;
    rdfs:comment "Indicates that an agent participated in a historical event, such as a battle, encounter, campaign, or gathering."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  san:Event .

#################################################################
# Property: hrao:wasPrisonerInEvent
#################################################################

hrao:wasPrisonerInEvent a owl:ObjectProperty ;
    rdfs:label "was taken prisoner in event"@en ;
    rdfs:comment "Indicates that an agent was taken prisoner during a particular historical event."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  san:Event .

#################################################################
# Property: hrao:killed
#################################################################

hrao:killed a owl:ObjectProperty ;
    rdfs:label "killed"@en ;
    rdfs:comment "Indicates that an agent caused the death of another agent, based on historical sources."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:wasKilledInEvent
#################################################################

hrao:wasKilledInEvent a owl:ObjectProperty ;
    rdfs:label "was killed in event"@en ;
    rdfs:comment "Indicates that an agent died as a direct consequence of a historical event, such as a battle, ambush, or other violent occurrence."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  san:Event .

#################################################################
# Property: hrao:servedUnder
#################################################################

hrao:servedUnder a owl:ObjectProperty ;
    rdfs:label "served under"@en ;
    rdfs:comment "Indicates that an agent held an office, function, or role under the authority or leadership of another agent."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:compadreOf
#################################################################

hrao:compadreOf a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "is compadre of"@en ;
    rdfs:comment "Indicates a mutual spiritual kinship relation between two persons, arising from the ritual of baptism or confirmation, common in Ibero-American societies."@en ;
    rdfs:subPropertyOf foaf:knows ;
    rdfs:domain foaf:Person ;
    rdfs:range  foaf:Person .

#################################################################
# Property: hrao:requestedCustodianshipFrom
#################################################################

hrao:requestedCustodianshipFrom a owl:ObjectProperty ;
    rdfs:label "requested custodianship from"@en ;
    rdfs:comment "Indicates that an agent formally requested to be appointed as custodian or depositary of goods, estates or documents, typically in administrative or legal contexts."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:appointedCuratorOf
#################################################################

hrao:appointedCuratorOf a owl:ObjectProperty ;
    rdfs:label "appointed as curator"@en ;
    rdfs:comment "Indicates that an agent with administrative or legal authority appointed another agent as curator or custodian of an estate, property, or goods, typically in probate or inheritance contexts."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:deliveredMessageTo
#################################################################

hrao:deliveredMessageTo a owl:ObjectProperty ;
    rdfs:label "delivered message to"@en ;
    rdfs:comment "Indicates that an agent acted as a messenger or courier by delivering a written message, letter, or communication to another agent, typically in historical or military correspondence contexts."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:wasCapturedIn
#################################################################

hrao:wasCapturedIn a owl:ObjectProperty ;
    rdfs:label "was captured in"@en ;
    rdfs:comment "Indicates the place where an agent was captured in a historical context."@en ;
    rdfs:subPropertyOf prov:atLocation ;
    rdfs:domain foaf:Agent ;
    rdfs:range  geo:SpatialThing .

#################################################################
# Property: hrao:wasImprisonedBy
#################################################################

hrao:wasImprisonedBy a owl:ObjectProperty ;
    rdfs:label "was imprisoned by"@en ;
    rdfs:comment "Indicates that an agent was imprisoned by another agent, typically a government, military authority, or institution."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:reportedTo
#################################################################

hrao:reportedTo a owl:ObjectProperty ;
    rdfs:label "reported to"@en ;
    rdfs:comment "Indicates that an agent reported information, orders, or status updates to another agent in a hierarchical or administrative context."@en ;
    rdfs:subPropertyOf prov:communicatedWith ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:issuedOrderToFree
#################################################################

hrao:issuedOrderToFree a owl:ObjectProperty ;
    rdfs:label "issued order to free"@en ;
    rdfs:comment "Indicates that an agent issued an order to release a prisoner or detainee."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:receivedPrisoners
#################################################################

hrao:receivedPrisoners a owl:ObjectProperty ;
    rdfs:label "received prisoners"@en ;
    rdfs:comment "Indicates that an agent received custody of one or more prisoners."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:deliveredPrisoners
#################################################################

hrao:deliveredPrisoners a owl:ObjectProperty ;
    rdfs:label "delivered prisoners"@en ;
    rdfs:comment "Indicates that an agent transferred prisoners to another agent or authority."@en ;
    rdfs:subPropertyOf prov:actedOnBehalfOf ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:transferredToCustodyOf
#################################################################

hrao:transferredToCustodyOf a owl:ObjectProperty ;
    rdfs:label "transferred to custody of"@en ;
    rdfs:comment "Indicates that an agent transferred a person or entity into the custody of another agent."@en ;
    rdfs:subPropertyOf prov:actedOnBehalfOf ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:wasTransferredTo
#################################################################

hrao:wasTransferredTo a owl:ObjectProperty ;
    rdfs:label "was transferred to"@en ;
    rdfs:comment "Indicates that an agent was transferred to the custody or authority of another agent."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:commanded
#################################################################

hrao:commanded a owl:ObjectProperty ;
    rdfs:label "commanded"@en ;
    rdfs:comment "Indicates that an agent exercised military or organizational command over another agent."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:coordinatedWith
#################################################################

hrao:coordinatedWith a owl:ObjectProperty ;
    rdfs:label "coordinated with"@en ;
    rdfs:comment "Indicates that two agents coordinated actions, decisions, or strategic movements in a historical context, especially in military or administrative settings."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:approached
#################################################################

hrao:approached a owl:ObjectProperty ;
    rdfs:label "approached"@en ;
    rdfs:comment "Indicates that an agent (often a military unit) moved toward or approached a specific place or strategic location."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  dcterms:Location .

#################################################################
# Property: hrao:retreatedTo
#################################################################

hrao:retreatedTo a owl:ObjectProperty ;
    rdfs:label "retreated to"@en ;
    rdfs:comment "Indicates that an agent (often a military force) withdrew or moved away from a location toward another location as a strategic or defensive action."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  dcterms:Location .

#################################################################
# Property: hrao:belongsToUnit
#################################################################

hrao:belongsToUnit a owl:ObjectProperty ;
    rdfs:label "belongs to military or administrative unit"@en ;
    rdfs:comment "Indicates that a person or agent is a member of, or formally belongs to, a military or administrative unit, corps, regiment, or organizational subdivision."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:hasKinAmong
#################################################################

hrao:hasKinAmong a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "has kin among"@en ;
    rdfs:comment "Indicates that an agent has relatives among a group, faction, or set of agents, without specifying the exact degree of kinship."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:hasChildrenAmong
#################################################################

hrao:hasChildrenAmong a owl:ObjectProperty ;
    rdfs:label "has children among"@en ;
    rdfs:comment "Indicates that an agent has one or more children within a particular group, faction, or set of agents."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:inConflictWith
#################################################################

hrao:inConflictWith a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "in conflict with"@en ;
    rdfs:comment "Indicates that two agents were in conflict, opposition, or antagonistic engagement within a historical context."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:hadPropertyConfiscatedBy
#################################################################

hrao:hadPropertyConfiscatedBy a owl:ObjectProperty ;
    rdfs:label "had property confiscated by"@en ;
    rdfs:comment "Indicates that a person or agent had their property confiscated, seized, or appropriated by another agent, typically in military or political conflict."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:requestedPaymentFor
#################################################################

hrao:requestedPaymentFor a owl:ObjectProperty ;
    rdfs:label "requested payment for"@en ;
    rdfs:comment "Indicates that an agent requested that another agent receive payment or financial compensation, typically in military or administrative contexts."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:heldPossessionOf
#################################################################

hrao:heldPossessionOf a owl:ObjectProperty ;
    rdfs:label "held possession of"@en ;
    rdfs:comment "Indicates that a person held long-term possession, use, or control of a property, land parcel, estate, or territorial unit in a historical context."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:hasKinship
#################################################################

hrao:hasKinship a owl:ObjectProperty ;
    rdfs:label "has kinship with"@en ;
    rdfs:comment "Indicates a family or kinship relation not covered by more specific relationship properties."@en ;
    rdfs:subPropertyOf rel:associateOf ;
    rdfs:domain foaf:Person ;
    rdfs:range  foaf:Person .

#################################################################
# Property: hrao:refusedToFollow
#################################################################

hrao:refusedToFollow a owl:ObjectProperty ;
    rdfs:label "refused to follow orders of"@en ;
    rdfs:comment "Indicates that an agent refused to comply with or obey the directives issued by another agent, particularly in a military or administrative context."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:defeatedInBattle
#################################################################

hrao:defeatedInBattle a owl:ObjectProperty ;
    rdfs:label "defeated in battle"@en ;
    rdfs:comment "Indicates that one agent militarily defeated another agent in battle, skirmish, or engagement."@en ;
    rdfs:subPropertyOf prov:influenced ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:wasReplacedBy
#################################################################

hrao:wasReplacedBy a owl:ObjectProperty ;
    rdfs:label "was replaced by"@en ;
    rdfs:comment "Indicates that an agent holding an office or command position was replaced by another agent, whether in military, political, or administrative contexts."@en ;
    rdfs:subPropertyOf prov:wasInfluencedBy ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent .

#################################################################
# Property: hrao:coerced
#################################################################

hrao:coerced a owl:ObjectProperty ;
    rdfs:label "coerced"@en ;
    rdfs:comment "Indicates that the active agent coerced or forced the passive agent to comply with a military or political action."@en ;
    rdfs:domain foaf:Agent ;
    rdfs:range  foaf:Agent ;
    rdfs:isDefinedBy hrao: .

#################################################################
# Property: hrao:socialKinship
#################################################################

hrao:socialKinship a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "has social kinship with"@en ;
    rdfs:comment """
Represents non-biological, non-marital kinship created through ritual,
social, or customary bonds such as compadrio (godparenthood), ritual
parenthood, fraternity bonds or other historically relevant forms of 
fictive kinship. The property is symmetric because these relations are
mutually acknowledged in historical records.
"""@en ;
    rdfs:subPropertyOf rel:relatedTo ;
    rdfs:domain foaf:Person ;
    rdfs:range  foaf:Person .

#################################################################
# Property: hrao:brotherInLawOf
#################################################################

hrao:brotherInLawOf a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:label "is brother-in-law of"@en ;
    rdfs:comment """
Represents an affinal kinship relation — brother-in-law — indicating that
two persons are related through marriage (e.g., spouse’s sibling or
sibling’s spouse). The property is symmetric because the relation holds
equally in both directions.
"""@en ;
    rdfs:subPropertyOf rel:relatedTo ;
    rdfs:domain foaf:Person ;
    rdfs:range  foaf:Person .